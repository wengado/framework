{
    "name": "wengado/framework",
    "description": "Wengado PHP Framework",
    "type": "library",
    "license": "MIT",
    "keywords": ["mvc", "framework"],
    "support": {
        "source": "https://github.com/wengado/framework"
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": { "Wengado\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Wengado\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    }
}
